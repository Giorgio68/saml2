{
    "name": "simplesamlphp/saml2",
    "description": "SAML2 PHP library from SimpleSAMLphp",
    "license": "LGPL-2.1-or-later",
    "authors": [
        {
            "name": "Andreas Åkre Solberg",
            "email": "andreas.solberg@uninett.no"
        },
        {
            "name": "Jaime Pérez Crespo",
            "email": "jaime.perez@uninett.no"
        },
        {
            "name": "Tim van Dijen",
            "email": "tvdijen@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-date": "*",
        "ext-dom": "*",
        "ext-openssl": "*",
        "ext-pcre": "*",
        "ext-zlib": "*",

        "nyholm/psr7": "^1.5.1",
        "psr/log": "^2.0 || ^3.0",
        "simplesamlphp/xml-common": "^1.7.0",
        "simplesamlphp/xml-security": "^1.6.0",
        "simplesamlphp/xml-soap": "^1.0.0",
        "simplesamlphp/assert": "^1.0.2"
    },
    "require-dev": {
        "mockery/mockery": "^1.5.1",
        "simplesamlphp/simplesamlphp-test-framework": "^1.4.1"
    },
    "autoload": {
        "psr-4": {
            "SimpleSAML\\SAML2\\": "src/SAML2"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SimpleSAML\\Test\\SAML2\\": "tests/SAML2/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "v5.0.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    }
}
